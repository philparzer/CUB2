{
    "name": "CUB2",
    "description": "New words shouldn't be intimidating",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
      },
    "permissions": ["storage", "activeTab", "scripting", "no-cors"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/cub2_logo16.png",
            "32": "/images/cub2_logo32.png",
            "48": "/images/cub2_logo48.png",
            "128": "/images/cub2_logo128.png"
          }
      },
    "icons": {
    "16": "/images/cub2_logo16.png",
    "32": "/images/cub2_logo32.png",
    "48": "/images/cub2_logo48.png",
    "128": "/images/cub2_logo128.png"
    },
    "options_page": "options.html"
  }